# ============================================
# Gallagher Property Company - AI Agent System
# Environment Configuration
# ============================================

# OpenAI Configuration (Required)
OPENAI_API_KEY=sk-...
OPENAI_ORG_ID=org-...

# OpenAI Model Configuration
# Flagship model for complex reasoning (Coordinator, Finance)
OPENAI_FLAGSHIP_MODEL=gpt-5.2
# Standard model for general tasks
OPENAI_STANDARD_MODEL=gpt-5.1
# Cost-effective model for simple tasks
OPENAI_MINI_MODEL=gpt-5.2-mini

# Perplexity API (Required for Research Agent)
PERPLEXITY_API_KEY=pplx-...
PERPLEXITY_MODEL=sonar-pro

# Supabase Configuration (Required)
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...
SUPABASE_ANON_KEY=eyJ...

# Google APIs (Required for Research Agent)
GOOGLE_MAPS_API_KEY=AIza...
GOOGLE_PLACES_API_KEY=AIza...
GOOGLE_SHEETS_API_KEY=AIza...
GOOGLE_DRIVE_API_KEY=AIza...

# Backblaze B2 (Required for document storage)
B2_APPLICATION_KEY_ID=...
B2_APPLICATION_KEY=...
B2_BUCKET_NAME=gallagher-documents
B2_ENDPOINT_URL=https://s3.us-west-000.backblazeb2.com

# Application Configuration
APP_ENV=development
APP_DEBUG=true
APP_LOG_LEVEL=INFO

# Agent Configuration
AGENT_MAX_TURNS=50
AGENT_TIMEOUT_SECONDS=300
AGENT_ENABLE_TRACING=true

# Louisiana/Market Specific
DEFAULT_MARKET_REGION=East Baton Rouge Parish
DEFAULT_STATE=Louisiana
DEFAULT_MSA=Greater Baton Rouge

# Feature Flags
ENABLE_WEB_SEARCH=true
ENABLE_FILE_SEARCH=true
ENABLE_CODE_INTERPRETER=true

# Tax Strategist (optional override)
# Defaults to prompts/references/IRC_Calculation_Logic_Library_2026.md
TAX_LIBRARY_PATH=
