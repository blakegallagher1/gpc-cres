{
  "name": "@entitlement-os/evidence",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"src/__tests__/**\" --max-warnings=0 --no-error-on-unmatched-pattern",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@entitlement-os/db": "workspace:*",
    "@entitlement-os/shared": "workspace:*",
    "@supabase/supabase-js": "^2.93.3",
    "html-to-text": "^9.0.5",
    "unpdf": "^1.4.0",
    "playwright": "^1.50.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
