{
  "name": "@entitlement-os/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "eslint prisma src seed --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.seed.json --noEmit",
    "test": "vitest run",
    "prisma": "dotenv -e ../../.env -- prisma",
    "generate": "dotenv -e ../../.env -- prisma generate",
    "migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy",
    "seed": "dotenv -e ../../.env -- tsx seed/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.20.1",
    "prisma": "^6.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "prisma": {
    "seed": "tsx seed/seed.ts"
  }
}
