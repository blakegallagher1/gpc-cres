{
  "name": "entitlement-os",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^2.1.9"
  },
  "scripts": {
    "dev": "pnpm -r --filter './packages/**' build && pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r --filter './packages/**' build && pnpm -r typecheck",
    "test": "pnpm -r test",
    "db:migrate": "pnpm -C packages/db run migrate:dev",
    "db:deploy": "pnpm -C packages/db run migrate:deploy",
    "db:seed": "pnpm -C packages/db run seed"
  }
}
